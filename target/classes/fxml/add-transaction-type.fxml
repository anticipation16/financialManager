<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.finman.controllers.AddTransactionTypeController">
    <children>
        <GridPane gridLinesVisible="true" layoutX="92.0" layoutY="103.0" prefHeight="123.0" prefWidth="416.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="177.0" minWidth="10.0" prefWidth="82.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="241.0" minWidth="10.0" prefWidth="241.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label prefHeight="17.0" prefWidth="149.0" text="Transaction Category">
                    <font>
                        <Font size="15.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets left="15.0"/>
                    </GridPane.margin>
                </Label>
                <Label prefHeight="17.0" prefWidth="149.0" text="Transation Type" GridPane.rowIndex="1">
                    <font>
                        <Font size="15.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets left="15.0"/>
                    </GridPane.margin>
                </Label>
                <TextField fx:id="transactionCategory" GridPane.columnIndex="1"/>
                <ChoiceBox fx:id="transactionTypeChoiceBox" prefHeight="26.0" prefWidth="242.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="1"/>
            </children>
        </GridPane>
        <Button layoutX="428.0" layoutY="265.0" mnemonicParsing="false" onAction="#handleAddTransactionType"
                prefHeight="38.0" prefWidth="161.0" text="Add Transaction Type"/>
        <Button layoutX="20.0" layoutY="26.0" mnemonicParsing="false" onAction="#handleBackButtonClick"
                prefHeight="38.0" prefWidth="161.0" text="Back To Add Transaction"/>
    </children>
</AnchorPane>
